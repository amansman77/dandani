{
  "name": "dandani-workspace",
  "version": "0.1.0",
  "private": true,
  "description": "단단이 프로젝트 워크스페이스",
  "workspaces": [
    "frontend",
    "workers"
  ],
  "scripts": {
    "install:all": "npm install && cd frontend && npm install && cd ../workers && npm install",
    "start:frontend": "cd frontend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "dev:workers": "cd workers && npm run dev",
    "deploy:workers": "cd workers && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amansman77/dandani.git"
  },
  "license": "Apache-2.0"
}
