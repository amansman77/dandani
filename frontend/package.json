{
  "name": "dandani",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@capacitor/app": "^8.0.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.15.10",
    "crypto-js": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sharp": "^0.34.2"
  },
  "scripts": {
    "start": "REACT_APP_API_URL=https://dandani-api.amansman77.workers.dev react-scripts start",
    "build": "REACT_APP_API_URL=https://dandani-api.amansman77.workers.dev react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cap:add": "npx cap add",
    "cap:sync": "npx cap sync",
    "cap:copy": "npx cap copy",
    "cap:update": "npx cap update",
    "cap:open": "npx cap open",
    "cap:run": "npx cap run",
    "cap:ios": "npm run build && npx cap sync && npx cap open ios",
    "cap:android": "npm run build && npx cap sync && npx cap open android",
    "icons:generate": "node scripts/generate-icons.js",
    "splash:generate": "node scripts/generate-splash.js",
    "assets:generate": "npm run icons:generate && npm run splash:generate",
    "xcode:clean": "bash scripts/clean-xcode-cache.sh",
    "android:clean": "bash scripts/fix-android-icon-cache.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/ios": "^8.0.0"
  }
}
